    <script type="text/javascript" language="javascript">
            var sessionId = Target.controller.header.cookie.read("estoreJSESSIONID"),
            userId = Target.controller.header.cookie.readUserActivityfromCookieForRR(sessionId);
            var pUserId ="";
            var searchTerm="";
            try{
            pUserId=HLLibrary._newGUID();
            }
            catch(e){
            }

            try{
            var cookieTmpVal = unescape(Target.controller.header.cookie.read("WC_PERSISTENT"));
             pUserId = cookieTmpVal.split("=")[0];
            }
            catch(e){
            }
            if(userId == "" ){
                  userId = "-1002";
            }
            if('' != ""){
                  userId = '';
            } 
            
             if('' != ""){
                  searchTerm = '';
				  searchTerm = searchTerm.replace("'","\\'");
            }
        HLLibrary.setProperty("clientId",  '131');
        HLLibrary.setProperty("pageType", 'product');
            HLLibrary.setProperty("cUserId", userId); 
            HLLibrary.setProperty("pUserId", pUserId);                   
        if('product' == "conf"){
                  HLLibrary.setProperty("prodId", '200141055');
                  HLLibrary.setProperty("prodP", '39.99');
                  HLLibrary.setProperty("ProdU", '');
                  HLLibrary.setProperty("oid", '');
                  HLLibrary.setProperty("oa", '');
            } else if('product' == "product"){
                  HLLibrary.setProperty("taxonomy", '4242');
                  HLLibrary.setProperty("prodId", '200141055');
                  HLLibrary.setProperty("qty", '1');
                  HLLibrary.setProperty("prodP", '39.99');
            } else if('product' == "category"){
                  HLLibrary.setProperty("taxonomy", searchTerm);
                  HLLibrary.setProperty("organicSKUs", ''); 
            }
            
            else{
              HLLibrary.setProperty("kw", searchTerm);
              HLLibrary.setProperty("view", '');
              HLLibrary.setProperty("pgSize", '');
              HLLibrary.setProperty("pCount", '');
              HLLibrary.setProperty("minOrganic", '0');
              HLLibrary.setProperty("minAds", '');

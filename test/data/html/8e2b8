<script type="text/javascript" charset="utf-8">
$(document).ready(function() {
	Target.controller.globalUtils.getDemoLinkURL();
		$('#OrderItemAddForm_200141055').validate({	
			focusInvalid:false,					
			errorPlacement: function (error, element) {					
				if(!error.text().length) return;			       
				
				if(element.hasClass('before-error-item')) {
					var errorElement = element.parents('.input-step');
					errorElement.before(error).prev().wrap('<div class="before-error-item"/>');
				}
				else if(element.hasClass('input-error-before')) {
					error.attr('role', 'alert').insertBefore(element);
				}
				else if(element.parents().hasClass('input-group')) {
					if(element.parents('.input-group').find('.input-group-title').length) {
						element.parents('.input-group').find('.input-group-title').after(error);
					} else if (element.parent().hasClass('input-step')) {
						element.parent().append(error);
					} else {
						element.parents('.input-group').prepend(error);
					}
				}
				else if(element.data('validator-group') || element.next().length) {
					element.parents('.input-step').append(error);
				}
				else if((element.is('input[type="radio"]') || element.is('input[type="checkbox"]')) && element.next('label').length) {
					element.next('label').after(error);
				}
				else if(element.is('textarea')) {
					element.prev('label').after(error);
				}
				else {
					error.insertAfter(element);
				}					
				
				var errorP = element.parents('form:first').find("p.error-message:first");						
				errorP.length ? errorP.attr("tabindex",1).focus() : '';		
			},				
			invalidHandler: function(form,validator) {							
				var errorP = $(this).find("p.error-message:first");				
				errorP.length ? errorP.attr("tabindex",1).focus() : '';	
			},							
			rules: {
				'attr-sizeSelection': 'required',
				'attr-sizeSelection2': 'required',
				'pdp-attr-swatchColor': 'required',
				'attr-send-type':'required',
				'quantity_0': {
					required: true,
					number: true,
					min:1
				},
				attributeType_1: {
					required : function(element) {
						var elem2	= $("#attributeType_2");											
						return (typeof elem2 != "undefined") ? true : false;
					}
				},
				attributeType_2: {
					required : function(element) {
						var elem1	= $("#attributeType_1");											
						return (typeof elem1 != "undefined") ? true : false;
					}
				}
			},
			messages: {
				'attr-sizeSelection': '',
				'attr-sizeSelection2': '',
				'pdp-attr-swatchColor': 'please choose a color.',
				'attr-send-type':'please choose sending option',
				'quantity_0': {
					required: 'At least 1 item is required.',
					number:	'invalid entry. please enter a number.',
					min: 'please enter valid quantity.'
				},
				'attributeType_1': 'please choose a size1',
				'attributeType_2': 'please choose a size2'
			},
			submitHandler: function(form) {
				var dropDowns = $("select[name=attr-sizeSelection]"),
					len = (typeof dropDowns != "undefined")? dropDowns.length : 0,
					dropDownVal = "";
				if(len > 1) {
					dropDownVal  = $("select#attributeType_2 option:selected").val();
					if(dropDownVal == "") return;
				}
				
	    		var qtyvl = $('#attr-quantity_200141055').val();
	    		$("#attr-quantity_1").val(qtyvl);
	    		$("#attr-quantity_2").val(qtyvl);
	    		
	    		
	    		var partnumberDoubleClk = 200141055;
	    		if(null != document.OrderItemAddForm_200141055.warranty 
	    				&& document.OrderItemAddForm_200141055.warranty.checked){
	    			var preOrderItem = false;
	    			var warrantyPrice = 0;
	    			var partnumber = 200141055+":"+document.OrderItemAddForm_200141055.warranty.value;
	    			partnumberDoubleClk = partnumberDoubleClk +"|"+document.OrderItemAddForm_200141055.warranty.value;
	    			
	    			if(null != document.OrderItemAddForm_200141055.warrantyPrice){
	    				warrantyPrice = document.OrderItemAddForm_200141055.warrantyPrice.value;
	    			}					    			
	    			var itemPrice = 39.99 * qtyvl+":"+warrantyPrice * qtyvl;
					var quantity = qtyvl+":"+qtyvl;
					var invCode = '0';	
					if(null != invCode && '' != invCode && ('4' == invCode || '5' == invCode)){
						preOrderItem = true;
					}				
					//s_cartAdd(partnumber,itemPrice+"",quantity+"",'Product Description Pages',preOrderItem);
				}
				
				var frequency = freqInterval = freqUnit = "";
				frequency = $("#babyDuration option:selected").val();
				if(frequency != "" && frequency != "undefined" && typeof frequency != "undefined"){
					frequency = frequency.split("-");
					$("#fulfillmentInterval").val(frequency[0]);
					$("#fulfillmentIntervalUOM").val(frequency[1]);
				}
				$('#subscribeme').is(':checked')?$("#recOrd").val('Y'):'';
				$("#recOrdStDt").val($("#babySubscribe").val());
				$("#recOrdfreq").val($("#babyDuration option:selected").html());			
				
				var formReg = $('form[name^="OrderItemAddForm_"]').find('#registry_Id');
				if( formReg.length !== 0 ) {
					Cart.add(form, true);
				} else {
					Cart.add(form);
				}
			}
		});
		$('form.no-summary').find('input#babySubscribe').accessibleCalendar({recurringPriceFlag:true}); 
	});
</script>




